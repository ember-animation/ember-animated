{
  "name": "ember-animated",
  "version": "0.10.1",
  "description": "Core animation primitives for Ember",
  "keywords": [
    "animation",
    "ember",
    "ember-addon",
    "transition"
  ],
  "repository": "https://github.com/ember-animation/ember-animated",
  "license": "MIT",
  "author": "Edward Faulkner <edward@eaf4.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint . --ext .ts,.js",
    "typecheck": "tsc --noEmit",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "prepublishOnly": "ember ts:precompile",
    "postpublish": "ember ts:clean"
  },
  "dependencies": {
    "assert-never": "^1.2.0",
    "ember-angle-bracket-invocation-polyfill": "^1.3.0 || ^2.0.0",
    "ember-auto-import": "^1.6.0",
    "ember-cli-babel": "^7.21.0",
    "ember-cli-htmlbars": "^5.2.0",
    "ember-cli-typescript": "^3.1.1",
    "ember-compatibility-helpers": "^1.2.0",
    "ember-decorators": "^6.1.1",
    "ember-element-helper": "^0.3.0",
    "ember-maybe-import-regenerator": "^0.1.5",
    "ember-named-arguments-polyfill": "^1.0.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^1.3.0",
    "@glimmer/component": "^1.0.1",
    "@glimmer/tracking": "^1.0.0",
    "@types/ember": "^3.1.1",
    "@types/ember-data": "^3.1.9",
    "@types/ember-qunit": "^3.4.7",
    "@types/ember-test-helpers": "^1.0.4",
    "@types/ember-testing-helpers": "^0.0.3",
    "@types/ember__test-helpers": "^0.7.9",
    "@types/qunit": "^2.9.0",
    "@types/rsvp": "^4.0.3",
    "@typescript-eslint/eslint-plugin": "^2.5.0",
    "@typescript-eslint/parser": "^2.5.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-animated-tools": "^0.3.0",
    "ember-cli": "~3.20.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-fastboot": "^2.2.3",
    "ember-cli-fastboot-testing": "^0.4.0",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sass": "^10.0.0",
    "ember-cli-typescript-blueprints": "^3.0.0",
    "ember-composable-helpers": "^2.2.0",
    "ember-concurrency": "^0.10.0",
    "ember-data": "3.10.0",
    "ember-decorators-polyfill": "^1.1.1",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-faker": "^1.2.1",
    "ember-load-initializers": "^2.1.1",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^8.0.0",
    "ember-source": "~3.20.2",
    "ember-source-channel-url": "^2.0.1",
    "ember-template-lint": "^2.9.1",
    "ember-truth-helpers": "^2.0.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-ember": "^8.9.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.1",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "qunit-dom": "^1.2.0",
    "sass": "^1.17.3",
    "typescript": "^3.7.3"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
