{
  "name": "ember-animated",
  "version": "0.11.0",
  "description": "Core animation primitives for Ember",
  "keywords": [
    "animation",
    "ember",
    "ember-addon",
    "transition"
  ],
  "repository": "https://github.com/ember-animation/ember-animated",
  "license": "MIT",
  "author": "Edward Faulkner <edward@eaf4.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache --ext .ts,.js",
    "lint:js:fix": "eslint . --fix --ext .ts,.js",
    "typecheck": "tsc --noEmit",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "prepublishOnly": "ember ts:precompile",
    "postpublish": "ember ts:clean"
  },
  "dependencies": {
    "@ember/test-helpers": "^2.6.0",
    "assert-never": "^1.2.0",
    "ember-angle-bracket-invocation-polyfill": "^1.3.0 || ^2.0.0 || ^3.0.1",
    "ember-auto-import": "^2.2.4",
    "ember-cli-babel": "^7.26.7",
    "ember-cli-htmlbars": "^5.7.2",
    "ember-cli-typescript": "^4.2.1",
    "ember-compatibility-helpers": "^1.2.0",
    "ember-element-helper": "^0.5.0",
    "ember-maybe-import-regenerator": "^0.1.5 || ^1.0.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@embroider/macros": "^0.50.0",
    "@glimmer/component": "^1.0.1",
    "@glimmer/tracking": "^1.0.0",
    "@types/ember": "^3.16.5",
    "@types/ember-data": "^3.16.15",
    "@types/ember-qunit": "^3.4.15",
    "@types/ember-test-helpers": "^1.0.10",
    "@types/ember-testing-helpers": "^0.0.4",
    "@types/ember__test-helpers": "^2.6.0",
    "@types/qunit": "^2.11.3",
    "@types/rsvp": "^4.0.4",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-animated-tools": "^0.3.0",
    "ember-cli": "~3.20.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-fastboot": "^2.2.3",
    "ember-cli-fastboot-testing": "^0.6.0",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sass": "^10.0.0",
    "ember-cli-typescript-blueprints": "^3.0.0",
    "ember-composable-helpers": "^5.0.0",
    "ember-concurrency": "^2.2.0",
    "ember-data": "3.10.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.1",
    "ember-qunit": "^5.1.5",
    "ember-resolver": "^8.0.0",
    "ember-source": "~3.20.2",
    "ember-source-channel-url": "^2.0.1",
    "ember-template-lint": "^4.0.0",
    "ember-truth-helpers": "^3.0.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ember": "^10.5.8",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-qunit": "^7.2.0",
    "faker": "5.5.3",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "qunit": "^2.17.2",
    "qunit-dom": "^2.0.0",
    "sass": "^1.17.3",
    "typescript": "^4.5.4",
    "webpack": "^5.65.0"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
