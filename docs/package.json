{
  "name": "ember-animated-docs",
  "version": "1.0.4",
  "private": true,
  "description": "Docs app for Ember Animated",
  "repository": "https://github.com/ember-animation/ember-animated",
  "license": "MIT",
  "author": "Edward Faulkner <edward@eaf4.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "deploy": "ember deploy production",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint \"**/*.css\" --allow-empty-input",
    "lint:css:fix": "concurrently \"npm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\"",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@ember-data/adapter": "^5.3.8",
    "@babel/eslint-parser": "^7.24.8",
    "@babel/plugin-proposal-decorators": "^7.23.9",
    "@ember/optional-features": "^2.2.0",
    "@ember/string": "^3.1.1",
    "@ember/test-helpers": "^3.3.0",
    "@faker-js/faker": "^8.4.1",
    "@fortawesome/ember-fontawesome": "^2.0.0",
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^9.1.0",
    "ember-animated": "workspace:*",
    "ember-animated-tools": "^2.0.0",
    "ember-auto-import": "^2.10.0",
    "ember-cli": "~6.2.0",
    "ember-cli-addon-docs": "^7.0.1",
    "ember-cli-addon-docs-yuidoc": "^1.1.0",
    "ember-cli-app-version": "^7.0.0",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-clean-css": "^3.0.0",
    "ember-cli-dependency-checker": "^3.3.2",
    "ember-cli-deploy": "^2.0.0",
    "ember-cli-deploy-build": "^3.0.0",
    "ember-cli-deploy-git": "^1.3.4",
    "ember-cli-deploy-git-ci": "^1.0.1",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sass": "^11.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-component-css": "^0.8.1",
    "ember-data": "~5.3.9",
    "ember-fetch": "^8.1.2",
    "ember-load-initializers": "^2.1.2",
    "ember-modifier": "^4.1.0",
    "ember-moment": "^10.0.1",
    "ember-qunit": "^8.0.2",
    "ember-resolver": "^11.0.1",
    "ember-simple-track-helper": "^2.0.0",
    "ember-source": "~5.6.0",
    "ember-template-lint": "^6.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-ember": "^12.3.3",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-qunit": "^8.1.1",
    "loader.js": "^4.7.0",
    "moment": "^2.30.1",
    "prettier": "^3.4.2",
    "qunit": "^2.20.1",
    "qunit-dom": "^3.0.0",
    "rollup": "^4.18.0",
    "sass": "^1.80.5",
    "stylelint": "^16.2.1",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-prettier": "^5.0.0",
    "tracked-built-ins": "^3.3.0",
    "webpack": "^5.90.3"
  },
  "engines": {
    "node": "16.* || >= 18"
  },
  "volta": {
    "extends": "../package.json"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "paths": [
      "../addon"
    ]
  }
}
