{
  "name": "ember-animated",
  "version": "1.0.4",
  "description": "Core animation primitives for Ember",
  "keywords": [
    "animation",
    "ember",
    "ember-addon",
    "transition"
  ],
  "homepage": "https://ember-animation.github.io/ember-animated/",
  "repository": {
    "type": "git",
    "url": "https://github.com/ember-animation/ember-animated.git",
    "directory": "addon"
  },
  "license": "MIT",
  "author": "Edward Faulkner <edward@eaf4.com>",
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*",
    "./test-support": "./dist/test-support/index.js",
    "./addon-main.js": "./addon-main.js"
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ],
      "*": [
        "./dist/*"
      ],
      "test-support": [
        "./dist/test-support/index.d.ts"
      ]
    }
  },
  "files": [
    "addon-main.js",
    "dist"
  ],
  "scripts": {
    "build": "rollup --config",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache --ext .ts,.js",
    "lint:js:fix": "eslint . --fix --ext .ts,.js",
    "prepublishOnly": "rollup --config",
    "start": "rollup --config --watch",
    "test": "echo 'Please refer to test-app for addon tests'",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@embroider/addon-shim": "^1.5.0",
    "@embroider/macros": "^1.5.0",
    "assert-never": "^1.2.1",
    "ember-element-helper": "^0.5.0 || ^0.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/plugin-proposal-decorators": "^7.17.9",
    "@babel/preset-typescript": "^7.16.7",
    "@embroider/addon-dev": "^1.6.0",
    "@glimmer/env": "^0.1.7",
    "@rollup/plugin-babel": "^5.3.1",
    "@types/ember": "^4.0.0",
    "@types/ember__object": "^4.0.1",
    "@types/rsvp": "^4.0.4",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "ember-template-lint": "^5.3.1",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-ember": "^10.6.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "rollup": "^2.70.2",
    "rollup-plugin-ts": "^2.0.7",
    "tslib": "^2.4.0",
    "typescript": "^4.6.3"
  },
  "peerDependencies": {
    "@ember/test-helpers": "^2.6.0"
  },
  "peerDependenciesMeta": {
    "@ember/test-helpers": {
      "optional": true
    }
  },
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "demoURL": "https://ember-animation.github.io/ember-animated/",
    "main": "addon-main.js",
    "type": "addon",
    "version": 2,
    "app-js": {
      "./components/animated-beacon.js": "./dist/_app_/components/animated-beacon.js",
      "./components/animated-container.js": "./dist/_app_/components/animated-container.js",
      "./components/animated-each.js": "./dist/_app_/components/animated-each.js",
      "./components/animated-if.js": "./dist/_app_/components/animated-if.js",
      "./components/animated-orphans.js": "./dist/_app_/components/animated-orphans.js",
      "./components/animated-value.js": "./dist/_app_/components/animated-value.js",
      "./components/ea-list-element.js": "./dist/_app_/components/ea-list-element.js",
      "./services/-ea-motion.js": "./dist/_app_/services/-ea-motion.js"
    }
  },
  "volta": {
    "extends": "../package.json"
  }
}
